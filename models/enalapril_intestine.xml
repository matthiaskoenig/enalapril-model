    <listOfRules>
      <assignmentRule name="absorption enalapril" variable="absorption">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <times/>
            <ci> ENAABS_k </ci>
            <ci> Vgu </ci>
            <ci> ena_lumen </ci>
          </apply>
        </math>
      </assignmentRule>
      <rateRule variable="PODOSE_ena">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <times/>
            <apply>
              <minus/>
              <ci> dissolution_ena </ci>
            </apply>
            <ci> Mr_ena </ci>
          </apply>
        </math>
      </rateRule>
    </listOfRules>
